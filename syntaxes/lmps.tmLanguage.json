			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numerics"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#var_brackets"
		},
		{
			"include": "#linecontinuation"
